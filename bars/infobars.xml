	<Button name="oUF_LocationInfoBar" inherits="oUF_LSInfoBarButtonTemplate-Long" frameStrata="LOW" parent="UIParent" enableMouse="true">
		<Anchors>
			<Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="6" y="-6"/>
		</Anchors>
		<Scripts>
			<OnClick>
				ToggleFrame(WorldMapFrame)
			</OnClick>
			<OnEnter>
				oUF_LocationInfoBar_OnEnter(self)
			</OnEnter>
			<OnUpdate>
				oUF_LocationInfoBar_OnUpdate(self, elapsed)
			</OnUpdate>
		</Scripts>
	</Button>
	<Button name="oUF_MemoryInfoBar" inherits="oUF_LSInfoBarButtonTemplate-Short" frameStrata="LOW" parent="UIParent" enableMouse="true">
		<Anchors>
			<Anchor point="LEFT" relativeTo="oUF_LocationInfoBar" relativePoint="RIGHT" x="24" y="0"/>
		</Anchors>
		<Scripts>
			<OnLoad>
				self.updateInterval = 0
				self.usedMemory = 0
				self.activeAddons = {}
			</OnLoad>
			<OnClick>
				oUF_MemoryInfoBar_OnClick(self)
			</OnClick>
			<OnEnter>
				oUF_MemoryInfoBar_OnEnter(self)
			</OnEnter>
			<OnUpdate>
				oUF_MemoryInfoBar_OnUpdate(self, elapsed)
			</OnUpdate>
		</Scripts>
	</Button>
