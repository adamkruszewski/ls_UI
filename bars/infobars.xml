	<Button name="oUF_BagsInfoBar" inherits="oUF_LSInfoBarButtonTemplate-Short" frameStrata="LOW" parent="UIParent" enableMouse="true">
		<Anchors>
			<Anchor point="LEFT" relativeTo="oUF_LatencyInfoBar" relativePoint="RIGHT" x="24" y="0"/>
		</Anchors>
		<Scripts>
			<OnLoad>
				self:RegisterForClicks("LeftButtonUp", "RightButtonUp")
				self:RegisterEvent("BAG_UPDATE")
			</OnLoad>
			<OnClick>
				oUF_BagsInfoBar_OnClick(self, button)
			</OnClick>
			<OnEvent>
				oUF_BagsInfoBar_OnEvent(self)
			</OnEvent>
		</Scripts>
	</Button>
	<Button name="oUF_ClockInfoBar" inherits="oUF_LSInfoBarButtonTemplate-Short" frameStrata="LOW" parent="UIParent" enableMouse="true">
