	<Button name="oUF_LSInfoBarButtonTemplate-Long" virtual="true">
		<Size>
			<AbsDimension x="170" y="40"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="0">
				<Texture name="$parentBackground" inherits="oUF_LSInfoBarLong-Background" parentkey="background">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BACKGROUND" textureSubLevel="1">
				<Texture name="$parentFilling" inherits="oUF_LSInfoBarLong-Filling" parentkey="filling">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
				<FontString name="$parentText" inherits="oUF_LSInfoBarText" parentkey="text">
					<Size>
						<AbsDimension x="148" y="20"/>
					</Size>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BORDER" textureSubLevel="0">
				<Texture name="$parentCover" inherits="oUF_LSInfoBarLong-Cover" parentkey="cover">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				self.updateInterval = 0
			</OnLoad>
			<OnLeave>
				GameTooltip:Hide()
			</OnLeave>
		</Scripts>
		<HighlightTexture name="$parentNormalTexture" inherits="oUF_LSInfoBarLong-Highlight">
			<Anchors>
				<Anchor point="CENTER"/>
			</Anchors>
		</HighlightTexture>
		<PushedTexture name="$parentPushedTexture" inherits="oUF_LSInfoBarLong-Pushed"/>
	</Button>

