<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Script file="buffs.lua"/>

	<Frame name="oUF_LSAuraHeaderTemplate" parent="UIParent" virtual="true">
		<Size>
			<AbsDimension x="30" y="30"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="0">
				<Texture name="$parentBackground" setAllPoints="true" hidden="true" parentKey="background">
					<Color r="1" g="1" b="1" a="1"/>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<Frame name="oUF_LSBuffHeader" inherits="oUF_LSAuraHeaderTemplate">
		<Anchors>
			<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-6" y="-60"/>
		</Anchors>
			<Scripts>
			<OnLoad>
				BuffFrame:SetParent(self)
				BuffFrame:ClearAllPoints()
				BuffFrame:SetPoint("TOPRIGHT", 0, 0)
			</OnLoad>
		</Scripts>
	</Frame>
	<Frame name="oUF_LSDebuffHeader" inherits="oUF_LSAuraHeaderTemplate">
		<Anchors>
			<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-6" y="-140"/>
		</Anchors>
	</Frame>
	<Frame name="oUF_LSTemporaryEnchantHeader" inherits="oUF_LSAuraHeaderTemplate">
		<Anchors>
			<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-6" y="-180"/>
		</Anchors>
			<Scripts>
			<OnLoad>
				TemporaryEnchantFrame:SetParent(self)
				TemporaryEnchantFrame:ClearAllPoints()
				TemporaryEnchantFrame:SetPoint("TOPRIGHT", 0, 0)
				UpdateTemporaryEnchantAnchors(self)
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>